<nav class="main-nav" id="main-nav">
    <%= link_to "Character Labs", root_path, id: "logo" %>
    <ul>
      <% if signed_in? %>
        <li><%= gravatar_for current_user %></li>
        <li class="user_name"><%= current_user.first_name %></li>
        <li><%= link_to "Grade Character Strengths", users_path %></li>
        <li><%= link_to "My Dashboard", users_path(current_user) %></li>
        <li><%= link_to "Settings", edit_user_path(current_user) %></li>
        <li><%= link_to "Sign out", signout_path, method: "delete" %></li>
      <% else %>
        <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
</nav>



